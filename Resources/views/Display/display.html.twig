{% extends admin_templates_base %}

{% block content %}
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-body">
                <table class="table table-bordered">
                    {% for property in display %}
                        <tr>
                            <td>
                                {% if property.label is not empty %}
                                {{ property.label }}
                                {% else %}
                                    -
                                {% endif %}
                            </td>
                            <td>{{ property.value }}</td>
                        </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
    </div>
{% endblock %}
